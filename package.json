{
    "name": "obsidian-plugin-prettier",
    "version": "1.0.2",
    "description": "Format your notes with Prettier and custom formatting options.",
    "main": "dist/main.js",
    "scripts": {
        "build": "rollup -c rollup.config.ts --configPlugin typescript",
        "dev": "rollup -c rollup.config.ts --configPlugin typescript --watch",
        "test": "vitest"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "prettier"
    ],
    "author": {
        "name": "GoodbyeNJN",
        "email": "cc@fuckwall.cc",
        "url": "https://github.com/GoodbyeNJN"
    },
    "license": "MIT",
    "dependencies": {
        "prettier": "^3.3.3"
    },
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^15.2.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@types/node": "^22.5.5",
        "dotenv": "^16.4.5",
        "eslint": "^8.57.0",
        "eslint-config-goodbyenjn": "^2.0.3",
        "fast-glob": "^3.3.2",
        "obsidian": "1.7.2",
        "rollup": "^4.22.4",
        "tslib": "^2.7.0",
        "typescript": "^5.6.2",
        "vite-tsconfig-paths": "^5.0.1",
        "vitest": "^2.1.1"
    },
    "obsidian": {
        "id": "prettier",
        "name": "Prettier",
        "isDesktopOnly": false,
        "minAppVersion": "0.10.12"
    }
}
