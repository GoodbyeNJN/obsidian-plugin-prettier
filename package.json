        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@types/node": "^22.7.4",
        "dotenv": "^16.4.5",
        "eslint": "^9.11.1",
        "eslint-config-goodbyenjn": "^3.1.0",
        "fast-glob": "^3.3.2",
        "jsx": "link:jsx",
